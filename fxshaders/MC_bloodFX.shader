gfx/impact/bloodstain1
{
	entityMergable
	surfaceparm nonsolid
	surfaceparm trans
	polygonOffset2
    {
        map gfx/impact/bloodstain1
        blendfunc blend
        rgbGen vertex
    nextbundle
		map $lightmap
    }
    {
		perlight
        map gfx/impact/bloodstain1
        blendfunc GL_SRC_ALPHA GL_ONE
        rgbGen vertex
    nextbundle
		map $dlight
    }
}

